#include<stdio.h> 
int Q[100],front=-1,rear=-1,x,i,n=5,choice; 
void Display(); 
void Insert(); 
void Delete(); 
void main(){ 
printf("Welcome to Travelista !!, Book your tickets now \n");; 
printf("You can avail waiting list facility\n"); 
do { 
printf("Enter 1.To Book Journey \t 2.To Cancel reservation\t 3.To Exit\n"); scanf("%d",&choice); 
switch(choice){ 
case 1:Insert(); 
break; 
case 2:Delete(); 
break; 
case 3:printf("Exited Succesfully\n"); 
break; 
} 
} 
while(choice!=3); 
}

void Insert(){ 
if(rear>=n-1) 
printf("Sorry, No Reservation available"); 
else{ 
printf("Enter Table number of choice[1-5] "); 
scanf("%d",&x); 
rear++; 
Q[rear]=x; 
if(front==-1){ 
front=0; 
} 
printf("Are you excited to travel %d spots ,Confirmed booking \n",x); } 
} 
void Delete(){ 
if (front==-1) 
{ 
printf("No Reservations to Cancel\n"); 
} 
else{ 
printf("The cancelled reservation is : %d\n",Q[front]); } 
if(front==rear) 
front=rear=-1; 
else 
front++; 
}
